{
  "name": "mangaanime-info-delivery-system",
  "version": "1.0.0",
  "description": "MangaAnime-Info-delivery-system with AI Development Environment",
  "main": "dist/index.js",
  "scripts": {
    "start": "node scripts/start-integrated.js",
    "dev": "nodemon scripts/start-integrated.js",
    "build": "tsc",
    "build:production": "tsc --project tsconfig.production.json",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "jest --coverage",
    "test:performance": "jest --testPathPattern=performance",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "security-audit": "npm audit",
    "docs:extract": "typedoc --out docs src",
    "docs:sync": "node scripts/sync-docs.js",
    "deploy:preview": "node scripts/deploy-preview.js",
    "deploy:production": "node scripts/deploy-production.js",
    "backup:production": "node scripts/backup.js",
    "notify:failure": "node scripts/notify.js failure",
    "notify:release": "node scripts/notify.js release",
    "claude-flow": "npx claude-flow@alpha swarm --claude",
    "init:all": "node scripts/init-all-systems.js",
    "status": "node scripts/system-status.js",
    "claude-start": "node /mnt/Linux-ExHDD/Claude-GlobalSettings/scripts/start-integrated.js",
    "claude-status": "node /mnt/Linux-ExHDD/Claude-GlobalSettings/scripts/system-status.js"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "js-yaml": "^4.1.0",
    "ws": "^8.0.0"
  },
  "optionalDependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "@octokit/auth-app": "^5.0.0",
    "@octokit/rest": "^19.0.0",
    "@octokit/webhooks": "^12.0.0",
    "sqlite": "^5.0.0",
    "sqlite3": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "playwright": "^1.55.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/integrated-ai-dev-env.git"
  },
  "keywords": [
    "ai",
    "development",
    "automation",
    "claude",
    "subagent",
    "claude-flow",
    "context7",
    "serena",
    "mcp",
    "hive-mind"
  ],
  "author": "AI Development Team",
  "license": "MIT"
}
